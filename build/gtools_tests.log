 96.         }
 97.     }
 98. 
.     if ( ("`table'" != "") | ("`benchmode'" != "") ) {
 99.         local rs = `time_pctile'  / `time_gpctile'
100.         di as txt "    `:di %6.3g `time_pctile'' | `:di %10.3g `time_gpctile'' | `:di %11.3g `rs'' | `anything' (`note')"
